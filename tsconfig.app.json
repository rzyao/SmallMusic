{
  "include": [
    "env.d.ts",
    "src/**/*",
    "src/global.d.ts",
    "src/**/*.vue",
    "env.musicApi.d.ts",
    "env.musicapi.development.ts",
    "plugins",
    "main.untils.js",
    "src/main/main.js"
  ],
  "exclude": [
    "src/**/__tests__/*",
    "src/**/*.spec.*",
    "src/**/*.test.*"
  ],
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "strict": true,
    "jsx": "preserve",
    "moduleResolution": "node",
    "composite": true,
    "baseUrl": ".",
    "verbatimModuleSyntax": true,
    "ignoreDeprecations": "5.0",
    "skipLibCheck": true,
    "paths": {
      "@/*": [
        "./src/*"
      ]
    }
  }
}